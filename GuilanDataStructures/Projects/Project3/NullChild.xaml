<UserControl
    x:Class="GuilanDataStructures.Projects.Project3.NullChild"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:GuilanDataStructures.Projects.Project3"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="control"
    Margin="-10,0"
    d:DesignHeight="35"
    d:DesignWidth="35"
    mc:Ignorable="d">
    <Canvas>
        <Ellipse
            Width="35"
            Height="35"
            Fill="#66DFDFDF"
            Stroke="Gray"
            StrokeDashArray="3" />
        <TextBlock
            x:Name="textBlock"
            Canvas.Left="3"
            Canvas.Top="3"
            Width="30"
            Height="30"
            FontFamily="{StaticResource Sahel}"
            FontSize="8"
            Foreground="Gray"
            TextAlignment="Center"
            TextWrapping="Wrap">
            Without Right
        </TextBlock>
    </Canvas>
</UserControl>
