<UserControl x:Class="GuilanDataStructures.Projects.Project3.NullChild"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GuilanDataStructures.Projects.Project3"
             mc:Ignorable="d" Margin="-10 0" x:Name="control"
             d:DesignHeight="35" d:DesignWidth="35">
    <Canvas>
        <Ellipse Width="35" Height="35" Stroke="Gray" StrokeDashArray="3" Fill="#66DFDFDF"/>
        <TextBlock x:Name="textBlock" Foreground="Gray" Canvas.Left="3" Canvas.Top="3" FontSize="8" TextAlignment="Center" Height="30" Width="30" TextWrapping="Wrap" FontFamily="{StaticResource Sahel}">بدون راست</TextBlock>
    </Canvas>
</UserControl>
